- title "Photos"

%table
  %tr
    %th
    %th Title
  - for photo in @photos
    %tr
      %td= image_tag photo.photo.url(:thumb)
      %td= h photo.title
      %td= link_to 'Show', photo
      %td= link_to 'Edit', edit_photo_path(photo)
      %td= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete

%p= link_to "New Photo", new_photo_path
